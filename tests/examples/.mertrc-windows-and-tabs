layout:
  -
    # Creates new window.
    split_strategy: "vertical"
    tabs:
      -
        layout:
          -
            - echo "Window 1, Tab 1"
      -
        split_strategy: "horizontal"
        layout:
          -
            - echo "Window 1, Tab 2, Pane 1"
          -
            - echo "Window 1, Tab 2, Pane 2"
  -
    # Creates new window.
    split_strategy: "vertical"
    layout:
      -
        - echo "Window 2, Tab 1, Pane 1"
      -
        - echo "Window 2, Tab 1, Pane 2"
